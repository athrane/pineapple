<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
		   xmlns="http://pineapple.dev.java.net/ns/support/docker_1_0" 
		   targetNamespace="http://pineapple.dev.java.net/ns/support/docker_1_0" 
		   elementFormDefault="qualified" 
		   attributeFormDefault="unqualified">

	<xs:element name="containerConfiguration" type="containerConfigurationType" />
	<xs:element name="createdContainer" type="createdContainerType" />
	<xs:element name="listedContainer" type="listedContainerType" />	
	<xs:element name="inspectedContainer" type="containerJsonType" />
	<xs:element name="inspectedImage" type="imageInspectType" />
	<xs:element name="imageCreationInfo" type="jsonMessageType" />
	<xs:element name="imageDeletionInfo" type="imageDeletionType" />
	<xs:element name="version" type="versionType" />

	<xs:complexType name="versionType" >
		<xs:annotation>
			<xs:documentation>Info about Docker version.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="version" type="xs:string" use="optional"/>		
		<xs:attribute name="os" type="xs:string" use="optional"/>		
		<xs:attribute name="kernelVersion" type="xs:string" use="optional"/>				
		<xs:attribute name="goVersion" type="xs:string" use="optional"/>				
		<xs:attribute name="gitCommit" type="xs:string" use="optional"/>		
		<xs:attribute name="arch" type="xs:string" use="optional"/>		
		<xs:attribute name="apiVersion" type="xs:string" use="optional"/>		
		<xs:attribute name="experimental" type="xs:string" use="optional"/>		
	</xs:complexType>

	<!-- Generic definitions -->

	<xs:simpleType name="stringList">
  		<xs:list itemType="xs:string"/>
	</xs:simpleType>

	<xs:simpleType name="intList">
  		<xs:list itemType="xs:int"/>
	</xs:simpleType>

	<xs:complexType name="errorType" >
		<xs:annotation>
			<xs:documentation>Generic error info .</xs:documentation>
		</xs:annotation>
		<xs:attribute name="message" type="xs:string" use="optional"/>		
	</xs:complexType>

	<!-- 
		Container for graphDriverDataType (implemented as map using JAXB binding)   
	 -->
	<xs:complexType name="graphDriverType" >
		<xs:annotation>
			<xs:documentation>Docker Graph driver configuration (Output from inspecting an image or container).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="data" type="graphDriverDataType" minOccurs="0" maxOccurs="1" />						
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="optional"/>		
	</xs:complexType>

	<!-- 
		Corresponds to GraphDriverData    
		https://github.com/moby/moby/blob/04295d26df3034a9a92da8400418f843d7c48780/api/types/blkiodev/blkio.go
	 -->
	<xs:complexType name="graphDriverDataType" >
		<xs:annotation>
			<xs:documentation>Docker graph driver data configuration (Output from inspecting an image or container).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="key" type="xs:string" /> <!-- key in map -->
			<xs:element name="value" type="xs:string" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="rootFSType" >
		<xs:annotation>
			<xs:documentation>Docker Root file system configuration  (Output from inspecting an image or container).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="layers" type="stringList" minOccurs="0" maxOccurs="1" />				
		</xs:sequence>
		<xs:attribute name="type" type="xs:string" use="optional"/>		
		<xs:attribute name="baseLayer" type="xs:string" use="optional"/>		
	</xs:complexType>

	<!-- 
		Container for weightDeviceType (implemented as default JAXB list)   
	 -->
	<xs:complexType name="weightDevicesType" >
		<xs:annotation>
			<xs:documentation>Container for device weight.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="devices" type="weightDeviceType" minOccurs="0" maxOccurs="unbounded" />
 		</xs:sequence>		
	</xs:complexType>

	<!-- 
		Corresponds to WeightDevice    
		https://github.com/moby/moby/blob/04295d26df3034a9a92da8400418f843d7c48780/api/types/blkiodev/blkio.go
	 -->
	<xs:complexType name="weightDeviceType" >
		<xs:annotation>
			<xs:documentation>Block IO weight (relative device weight) (Output from inspecting an image or container).</xs:documentation>
		</xs:annotation>
		<xs:attribute name="path" type="xs:string" use="optional"/>		
		<xs:attribute name="weight" type="xs:int" use="optional"/> <!-- approximation of the Weight which is implemented as a uint16 -->		
	</xs:complexType>

	<!-- 
		Container for throttleDeviceType (implemented as default JAXB list)   
	 -->
	<xs:complexType name="throttleDevicesType" >
		<xs:annotation>
			<xs:documentation>Container for device throttling.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="devices" type="throttleDeviceType" minOccurs="0" maxOccurs="unbounded" />
 		</xs:sequence>		
	</xs:complexType>

	<!-- 
		Corresponds to ThrottleDevice     
		https://github.com/moby/moby/blob/04295d26df3034a9a92da8400418f843d7c48780/api/types/blkiodev/blkio.go
	 -->
	<xs:complexType name="throttleDeviceType" >
		<xs:annotation>
			<xs:documentation>Limit read rate (bytes per second) from a device (Output from inspecting an image or container).</xs:documentation>
		</xs:annotation>
		<xs:attribute name="path" type="xs:string" use="optional"/>		
		<xs:attribute name="rate" type="xs:int" use="optional"/> <!-- approximation of the Rate which is implemented as a uint16 -->		
	</xs:complexType>

	<!-- 
		Corresponds to Mount   
		https://github.com/moby/moby/blob/04295d26df3034a9a92da8400418f843d7c48780/api/types/mount/mount.go		

		Please notice:
		
		The type for "propagation" is defined as string in 		
		https://github.com/docker/docker/blob/04295d26df3034a9a92da8400418f843d7c48780/api/types/mount/mount.go
		
		The type for "type" is defined as string in 		
		https://github.com/docker/docker/blob/04295d26df3034a9a92da8400418f843d7c48780/api/types/mount/mount.go		
	 -->
	<xs:complexType name="mountType">
		<xs:annotation>
			<xs:documentation>Container mount point (Output from listing a container).</xs:documentation>
		</xs:annotation>		
		<xs:attribute name="destination" type="xs:string" use="optional"/>								
		<xs:attribute name="driver" type="xs:string" use="optional"/>								
		<xs:attribute name="mode" type="xs:string" use="optional"/>								
		<xs:attribute name="name" type="xs:string" use="optional"/>										
		<xs:attribute name="propagation" type="xs:string" use="optional"/>								
		<xs:attribute name="source" type="xs:string" use="optional"/>									
		<xs:attribute name="type" type="xs:string" use="optional"/>												
		<xs:attribute name="rw" type="xs:boolean" use="optional"/>										
	</xs:complexType>

	<!-- 
		Corresponds to /pkg/jsonmessage/JSONProgress
		https://github.com/moby/moby/blob/master/pkg/jsonmessage/jsonmessage.go
	-->
	<xs:complexType name="jsonProgressType" >
		<xs:annotation>
			<xs:documentation>JSONProgress describes a Progress..</xs:documentation>
		</xs:annotation>
		<xs:attribute name="current" type="xs:long" use="optional"/> <!-- approximation of the Current which is implemented as a int64 -->				
		<xs:attribute name="start" type="xs:long" use="optional"/> <!-- approximation of the Start which is implemented as a int64 -->						
		<xs:attribute name="hideCounts" type="xs:boolean" use="optional"/> 				
		<xs:attribute name="total" type="xs:long" use="optional"/> <!-- approximation of the Total which is implemented as a int64 -->				
		<xs:attribute name="units" type="xs:string" use="optional"/> 				
	</xs:complexType>

	<!-- 
		Corresponds to /pkg/jsonmessage/JSONError
		https://github.com/moby/moby/blob/master/pkg/jsonmessage/jsonmessage.go
	-->
	<xs:complexType name="jsonErrorType" >
		<xs:annotation>
			<xs:documentation>JSONError wraps a concrete Code and Message, Code is is an integer error code, Message is the error message.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="code" type="xs:int" use="optional"/> 				
		<xs:attribute name="message" type="xs:string" use="optional"/> 			
	</xs:complexType>

	<!-- 
		Corresponds to /nat/PortSet 
		https://github.com/docker/go-connections/blob/master/nat/nat.go
	-->
	<xs:complexType name="portSetType">
		<xs:annotation>
			<xs:documentation>PortSet is a collection of structs indexed by Port.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="elements" type="portSetElementType" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="portSetElementType">
		<xs:annotation>
			<xs:documentation>
				Element in port set.
				The value isn't used by Docker, but a null value (an object) must be defined.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="port" type="xs:string" />
			<xs:element name="nullValue" type="portSetElementNullValueType" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="portSetElementNullValueType">
		<xs:annotation>
			<xs:documentation>Null value for port in port set.</xs:documentation>
		</xs:annotation>
	</xs:complexType>

	<!-- 
		Corresponds to /nat/PortMap 
		https://github.com/docker/go-connections/blob/master/nat/nat.go
	-->
	<xs:complexType name="portMapType" >
		<xs:annotation>
			<xs:documentation>PortMap is a collection of PortBinding indexed by Port.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="elements" type="portMapElementType" minOccurs="0" maxOccurs="unbounded" />
 		</xs:sequence>		
	</xs:complexType>

	<xs:complexType name="portMapElementType">
		<xs:annotation>
			<xs:documentation>Port map element. The value is a port binding.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="port" type="xs:string" minOccurs="0" maxOccurs="1" />
			<xs:element name="binding" type="portBindingType" minOccurs="0" maxOccurs="unbounded" />						
		</xs:sequence>
	</xs:complexType>
	
	<!-- 
		Corresponds to /nat/PortBinding 
		https://github.com/docker/go-connections/blob/master/nat/nat.go
	-->
	<xs:complexType name="portBindingType">
		<xs:annotation>
			<xs:documentation>PortBinding represents a binding between a Host IP address and a Host Port.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="hostIp" type="xs:string" /> 
			<xs:element name="hostPort" type="xs:string" />
		</xs:sequence>
	</xs:complexType>

	<!-- Image definitions -->

	<xs:complexType name="listedImageType" >
		<xs:annotation>
			<xs:documentation>Info about a listed Docker image (Output from the "List images" operation).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="labels" type="containerConfigurationLabelsType" minOccurs="0" maxOccurs="1" />					
			<xs:element name="repoTags" type="stringList" minOccurs="0" maxOccurs="1" />			
			<xs:element name="repoDigests" type="stringList" minOccurs="0" maxOccurs="1" />			
		</xs:sequence>
		<xs:attribute name="containers" type="xs:integer" use="optional"/>		
		<xs:attribute name="created" type="xs:integer" use="optional"/>				
		<xs:attribute name="id" type="xs:string" use="optional"/>		
		<xs:attribute name="parentId" type="xs:string" use="optional"/>				
		<xs:attribute name="size" type="xs:integer" use="optional"/>		
		<xs:attribute name="sharedSize" type="xs:integer" use="optional"/>				
		<xs:attribute name="virtualSize" type="xs:integer" use="optional"/>		
	</xs:complexType>

	<!-- 
		Corresponds to ImageInspect
		https://github.com/moby/moby/blob/master/api/types/types.go
	 -->
	<xs:complexType name="imageInspectType" >
		<xs:annotation>
			<xs:documentation>Info about an inspected Docker image (Output from the "Inspect an image" operation).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="containerConfig" type="containerConfigurationType" minOccurs="0" maxOccurs="1" />
			<xs:element name="config" type="containerConfigurationType" minOccurs="0" maxOccurs="1" />
			<xs:element name="graphDriver" type="graphDriverType" minOccurs="0" maxOccurs="1" />			
			<xs:element name="repoDigests" type="stringList" minOccurs="0" maxOccurs="1" />						
			<xs:element name="repoTags" type="stringList" minOccurs="0" maxOccurs="1" />								
			<xs:element name="rootFS" type="rootFSType" minOccurs="0" maxOccurs="1" />						
 		</xs:sequence>
		<xs:attribute name="author" type="xs:string" use="optional"/>		
		<xs:attribute name="architecture" type="xs:string" use="optional"/>		
		<xs:attribute name="created" type="xs:string" use="optional"/>		
		<xs:attribute name="comment" type="xs:string" use="optional"/>		
		<xs:attribute name="container" type="xs:string" use="optional"/>
		<xs:attribute name="dockerVersion" type="xs:string" use="optional"/>
		<xs:attribute name="id" type="xs:string" use="optional"/>		
		<xs:attribute name="os" type="xs:string" use="optional"/>		
		<xs:attribute name="osVersion" type="xs:string" use="optional"/>				
		<xs:attribute name="parent" type="xs:string" use="optional"/>		
		<xs:attribute name="size" type="xs:long" use="optional"/>	
		<xs:attribute name="virtualSize" type="xs:long" use="optional"/>	
	</xs:complexType>

	<!-- 
		Corresponds to /pkg/jsonmessage/JSONMessage     
		https://github.com/moby/moby/blob/eb8abc95985bf3882a4a177c409a96e36e25f5b7/pkg/jsonmessage/jsonmessage.go#L119	 
	-->
	<xs:complexType name="jsonMessageType" >
		<xs:annotation>
			<xs:documentation>It describes the created time, where it from, status, ID of the message. It's used for docker events.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="progressDetail" type="jsonProgressType" minOccurs="0" maxOccurs="1" />
			<xs:element name="errorDetail" type="jsonErrorType" minOccurs="0" maxOccurs="1" />
		</xs:sequence>
		<xs:attribute name="error" type="xs:string" use="optional"/>				
		<xs:attribute name="from" type="xs:string" use="optional"/>		
		<xs:attribute name="id" type="xs:string" use="optional"/>		
		<xs:attribute name="progress" type="xs:string" use="optional"/>		
		<xs:attribute name="status" type="xs:string" use="optional"/>		
		<xs:attribute name="stream" type="xs:string" use="optional"/>		
		<xs:attribute name="time" type="xs:long" use="optional"/> <!-- approximation of the Time which is implemented as a int64 -->				
		<xs:attribute name="timeNano" type="xs:long" use="optional"/> <!-- approximation of the TimeNano which is implemented as a int64 -->						
	</xs:complexType>

	<xs:complexType name="imageDeletionType" >
		<xs:annotation>
			<xs:documentation>Info about Docker image deletion progress.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="untagged" type="xs:string" use="optional"/>		
		<xs:attribute name="deleted" type="xs:string" use="optional"/>		
	</xs:complexType>

	<!-- Container definitions -->

	<!-- 
		Corresponds to Config    
		https://github.com/moby/moby/blob/master/api/types/container/config.go
	 -->
	<xs:complexType name="containerConfigurationType" >
		<xs:annotation>
			<xs:documentation>Docker container configuration (Input to container creation).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="cmd" type="stringList" minOccurs="0" maxOccurs="1" />
			<xs:element name="entrypoint" type="stringList" minOccurs="0" maxOccurs="1" />
			<xs:element name="env" type="stringList" minOccurs="0" maxOccurs="1" />			
			<xs:element name="exposedPorts" type="portSetType" minOccurs="0" maxOccurs="1" />		
			<xs:element name="healthcheck" type="HealthConfigType" minOccurs="0" maxOccurs="1" />				
			<xs:element name="hostConfig" type="containerConfigurationHostConfigType" minOccurs="0" maxOccurs="1" />			
			<xs:element name="labels" type="containerConfigurationLabelsType" minOccurs="0" maxOccurs="1" />			
			<xs:element name="onBuild" type="stringList" minOccurs="0" maxOccurs="1" />
			<xs:element name="shell" type="stringList" minOccurs="0" maxOccurs="1" />
			<xs:element name="volumes" type="containerConfigurationVolumesType" minOccurs="0" maxOccurs="1" />						
		</xs:sequence>
		<xs:attribute name="argsEscaped" type="xs:boolean" use="optional"/>				
		<xs:attribute name="attachStderr" type="xs:boolean" use="optional"/>		
		<xs:attribute name="attachStdin" type="xs:boolean" use="optional"/>		
		<xs:attribute name="attachStdout" type="xs:boolean" use="optional"/>		
		<xs:attribute name="domainname" type="xs:string" use="optional"/>				
		<xs:attribute name="hostname" type="xs:string" use="optional"/>		
		<xs:attribute name="image" type="xs:string" use="optional"/>				
		<xs:attribute name="macAddress" type="xs:string" use="optional"/>		
		<xs:attribute name="networkDisabled" type="xs:boolean" use="optional"/>				
		<xs:attribute name="openStdin" type="xs:boolean" use="optional"/>			
		<xs:attribute name="stdinOnce" type="xs:boolean" use="optional"/>	
		<xs:attribute name="stopSignal" type="xs:string" use="optional"/>	
		<xs:attribute name="stopTimeout" type="xs:int" use="optional"/>							
		<xs:attribute name="tty" type="xs:boolean" use="optional"/>				
		<xs:attribute name="user" type="xs:string" use="optional"/>		
		<xs:attribute name="workingDir" type="xs:string" use="optional"/>		
	</xs:complexType>

	<xs:complexType name="containerConfigurationLabelsType" >
		<xs:annotation>
			<xs:documentation>Info about labels in a container configuration and inspected container.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="label" type="containerConfigurationLabelType" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>		
	</xs:complexType>

	<xs:complexType name="containerConfigurationLabelType">
		<xs:annotation>
			<xs:documentation>Label in container configuration and inspected container.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="key" type="xs:string" /> <!-- key in map -->
			<xs:element name="value" type="xs:string" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="containerConfigurationVolumesType">
		<xs:annotation>
			<xs:documentation>Info about volumes in a container configuration.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="volume" type="containerConfigurationVolumeType" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="containerConfigurationVolumeType">
		<xs:annotation>
			<xs:documentation>
				Volume in container configuration. 
				The value isn't used by Docker, but a null value (an object) must be defined.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="mountpoint" type="xs:string" /> <!--  key in map -->
			<xs:element name="nullValue" type="containerConfigurationVolumeNullValueType" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="containerConfigurationVolumeNullValueType">
		<xs:annotation>
			<xs:documentation>Null value for a volume in container configuration.</xs:documentation>
		</xs:annotation>
	</xs:complexType>

	<!-- 
		Corresponds to HealthConfig   
		https://github.com/moby/moby/blob/04295d26df3034a9a92da8400418f843d7c48780/api/types/container/config.go#L11
	-->
	<xs:complexType name="HealthConfigType" >
		<xs:annotation>
			<xs:documentation>HealthConfig holds configuration settings for the healthcheck feature.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="test" type="stringList" minOccurs="0" maxOccurs="1" />
 		</xs:sequence>		
		<xs:attribute name="interval" type="xs:long" use="optional"/>  <!-- approximation of the Interval which is implemented as a time.Duration -->				
		<xs:attribute name="timeout" type="xs:long" use="optional"/>  <!-- approximation of the Timeout which is implemented as a time.Duration -->				
		<xs:attribute name="Retries" type="xs:int" use="optional"/>  	
	</xs:complexType>

	<!-- 
		Corresponds to HostConfig   
		https://github.com/moby/moby/blob/master/api/types/container/host_config.go

		Please notice:
		
		The type for "cgroup" is defined as string in 		
		https://github.com/docker/docker/blob/04295d26df3034a9a92da8400418f843d7c48780/api/types/mount/mount.go		
		
		The type for "usernsMode" is defined as string in 		
		https://github.com/moby/moby/blob/04295d26df3034a9a92da8400418f843d7c48780/api/types/container/host_config.go
		
		The type for "Isolation" is defined as string in 		
		https://github.com/moby/moby/blob/04295d26df3034a9a92da8400418f843d7c48780/api/types/container/host_config.go
		
	 -->
	<xs:complexType name="containerConfigurationHostConfigType" >
		<xs:annotation>
			<xs:documentation>Info about host configuration in a container configuration.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="binds" type="stringList" minOccurs="0" maxOccurs="1" /> 			
			<xs:element name="consoleSize" type="intList" minOccurs="0" maxOccurs="1" /> 			
			<xs:element name="dns" type="stringList" minOccurs="0" maxOccurs="1" /> 
			<xs:element name="dnsOptions" type="stringList" minOccurs="0" maxOccurs="1" /> 						
			<xs:element name="dnsSearch" type="stringList" minOccurs="0" maxOccurs="1" />	
			<xs:element name="extraHosts" type="stringList" minOccurs="0" maxOccurs="1" />	
			<xs:element name="groupAdd" type="stringList" minOccurs="0" maxOccurs="1" />	
			<xs:element name="links" type="stringList" minOccurs="0" maxOccurs="1" />				
			<xs:element name="restartPolicy" type="containerConfigurationHostConfigRestartPolicyType" minOccurs="0" maxOccurs="1" />
			<xs:element name="logConfig" type="containerConfigurationHostConfigLogConfigType" minOccurs="0" maxOccurs="1" />
			<xs:element name="portBindings" type="portMapType" minOccurs="0" maxOccurs="1" />
			<xs:element name="blkioWeightDevice" type="weightDevicesType" minOccurs="0" maxOccurs="1" />
			<xs:element name="blkioDeviceReadBps" type="throttleDevicesType" minOccurs="0" maxOccurs="1" />
			<xs:element name="blkioDeviceWriteBps" type="throttleDevicesType" minOccurs="0" maxOccurs="1" />
			<xs:element name="blkioDeviceReadIOps" type="throttleDevicesType" minOccurs="0" maxOccurs="1" />
			<xs:element name="blkioDeviceWriteIOps" type="throttleDevicesType" minOccurs="0" maxOccurs="1" />
			<xs:element name="securityOpt" type="stringList" minOccurs="0" maxOccurs="1" />	
			<xs:element name="volumesFrom" type="stringList" minOccurs="0" maxOccurs="1" />				
 		</xs:sequence>
		<xs:attribute name="autoRemove" type="xs:boolean" use="optional"/>		 		 		 		
		<xs:attribute name="blkioWeight" type="xs:long" use="optional"/> <!-- Not implemented yet-->		
		<xs:attribute name="capAdd" type="xs:string" use="optional"/> <!-- wrong type -->		
		<xs:attribute name="capDrop" type="xs:string" use="optional"/> <!-- wrong type -->
		<xs:attribute name="cgroup" type="xs:string" use="optional"/> <!-- approximation of the CgroupSpec which is implemented as a string -->					
		<xs:attribute name="cgroupParent" type="xs:string" use="optional"/> 		
